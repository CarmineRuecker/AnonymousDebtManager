{
  "name": "anonymous-debt-restructuring",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Privacy-first debt management platform using blockchain encryption technology",
  "keywords": [
    "debt-management",
    "privacy",
    "anonymity",
    "restructuring",
    "blockchain",
    "encryption",
    "defi",
    "financial-privacy",
    "web3",
    "ethereum",
    "sepolia",
    "react",
    "typescript",
    "vite"
  ],
  "author": "Anonymous Debt Restructuring Platform",
  "license": "MIT",
  "homepage": "https://anonymous-debt-restructuring.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/username/anonymous-debt-restructuring.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\"",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "deploy": "hardhat run scripts/deploy.ts",
    "deploy:localhost": "hardhat run scripts/deploy.ts --network localhost",
    "deploy:sepolia": "hardhat run scripts/deploy.ts --network sepolia",
    "deploy:fhevm": "hardhat run scripts/deploy.ts --network fhevm",
    "verify": "hardhat verify",
    "node": "hardhat node",
    "clean": "hardhat clean",
    "coverage": "hardhat coverage"
  },
  "dependencies": {
    "@types/node": "22.8.1",
    "ethers": "6.13.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "@fhevm/fhevm-js": "^0.7.0",
    "fhenixjs": "^0.2.0"
  },
  "devDependencies": {
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "8.13.0",
    "@typescript-eslint/parser": "8.13.0",
    "@vitejs/plugin-react": "4.3.3",
    "eslint": "8.57.1",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-plugin-react-refresh": "0.4.14",
    "prettier": "3.3.3",
    "typescript": "5.6.3",
    "vite": "6.0.1",
    "@fhevm/solidity": "^0.7.0",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.8",
    "@nomicfoundation/hardhat-ethers": "^3.0.8",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.12",
    "@nomicfoundation/hardhat-verify": "^2.0.11",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "hardhat": "^2.22.17",
    "hardhat-gas-reporter": "^2.2.1",
    "solidity-coverage": "^0.8.13",
    "typechain": "^8.3.2",
    "chai": "^4.5.0",
    "mocha": "^10.8.2",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.9"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
